{
  "name": "TEMPLATE_NAME",
  "emitLegacyScripts": false,
  "tree": {
    "$className": "DataModel",
    "Workspace": { "$properties": { "StreamingEnabled": true } },
    "Players": { "$properties": { "CharacterAutoLoads": true } },
    "Lighting": {
      "$properties": { "GlobalShadows": true, "Technology": "Future" }
    },
    "ReplicatedStorage": {
      "$path": "src/Shared",
      "Client": { "$path": "src/Client" },
      "Server": { "$path": "src/Server" },
      "Assets": { "$path": "src/Assets" },
      "Packages": { "$path": "Packages" },
      "Events": { "$path": "src/Events" },
      "Modules": { "$path": "src/Modules" },
      "Data": { "$path": "src/Data" },
      "Ui": { "$path": "src/Ui" },
      "Utils": { "$path": "src/Utils" }
    },
    "ServerStorage": { "$path": "src/Storage" },
    "StarterPlayer": {
      "$properties": {
        "CameraMinZoomDistance": 0.5,
        "CameraMaxZoomDistance": 0.5
      },
      "StarterCharacterScripts": {
        "Animate": {
          "$className": "Script",
          "$properties": { "RunContext": "Legacy", "Enabled": true }
        },
        "Health": {
          "$className": "Script",
          "$properties": { "RunContext": "Legacy", "Enabled": true }
        }
      },
      "StarterPlayerScripts": {}
    },
    "HttpService": { "$properties": { "HttpEnabled": true } },
    "TextChatService": { "$properties": { "ChatVersion": "TextChatService" } }
  }
}
